#include <iostream>
#include <fstream>
#include <string>
#include "person.pb.h"
using namespace std;

int main(int argc, char* argv[]) {
  GOOGLE_PROTOBUF_VERIFY_VERSION;

  phonebook::Person person;
  person.set_firstname("John");
  person.set_lastname("Doe");

  cout << person.get_firstname() << end;
}
